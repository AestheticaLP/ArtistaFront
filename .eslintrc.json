{
  "extends": ["airbnb"],
  "plugins": ["@typescript-eslint", "react"],
  "parser": "@typescript-eslint/parser",
  "ignorePatterns": ["__tests__/App-test.tsx"],
  "rules": {
    "import/no-unresolved": 0,
    "no-tabs": 0,
    "indent": "off",
    "react/jsx-indent": "off",
    "react/jsx-filename-extension": [1, {
      "extensions": [
        ".jsx",
        ".tsx"
      ]
    }]
  }
}